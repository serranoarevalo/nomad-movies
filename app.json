{
  "expo": {
    "name": "Nomad Movies",
    "githubUrl": "https://github.com/serranoarevalo/nomad-movies",
    "primaryColor": "#000000",
    "description": "Beautiful App to find Movies and TV Shows",
    "slug": "nomad-movies",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconCorn.png",
    "splash": {
      "image": "./assets/splashOne.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "co.nomadcoders.nomadmovies",
      "buildNumber": "1.0.0"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "android": {
      "package": "co.nomadcoders.nomadmovies"
    }
  }
}
